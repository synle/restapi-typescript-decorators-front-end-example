{
  "name": "restapi-typescript-decorators-front-end-example",
  "version": "1.0.0",
  "description": "![Build and Test CI](https://github.com/synle/restapi-typescript-decorators-example/workflows/Build%20and%20Test%20CI/badge.svg)",
  "main": "webpack.config.js",
  "dependencies": {
    "react": "^17",
    "react-dom": "^17",
    "restapi-typescript-decorators": "^7"
  },
  "devDependencies": {
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.10",
    "typescript": "^4"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist ./build",
    "start": "SASS_PATH='./src/styles' parcel src/index.html --port 3000 --out-dir build",
    "build": "SASS_PATH='./src/styles' parcel build src/index.html --out-dir dist --public-url ./",
    "format": "npx prettier --config ./.prettierrc --write src/**/**/**/**/*.{ts,tsx,js,jsx,scss,yml} *.{json,MD}",
    "lint": "eslint --ext js,jsx,ts,tsx src",
    "test": "echo \"Error: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synle/restapi-typescript-decorators-front-end-example.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/synle/restapi-typescript-decorators-front-end-example/issues"
  },
  "homepage": "https://github.com/synle/restapi-typescript-decorators-front-end-example#readme"
}
